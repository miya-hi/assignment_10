<h2><%= t 'admin.labels.index.labels' %></h2>
<p class="new_task"><%= link_to (t 'admin.labels.index.to_new_page'), new_admin_label_path %></p>
<table>
<% @labels.each do |label| %>
  <tr><td><%= label.name %></td>
  <td><%= link_to (t 'admin.labels.index.destroy'), admin_label_path(label.id), method: :delete, data: { confirm: '削除してよろしいですか？'} %></td>
  </tr>
<% end %>
</table>
